    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/api-server/main.go"
        },
        {
            "name": "Launch Migration",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/migrate/main.go",
            "args": [
                "-m",
                "migrate"
            ]
        },
        {
            "name": "Launch Seed",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/migrate/main.go",
            "args": [
                "-m",
                "seed"
            ]
        },
        {
            "name": "Launch Refresh",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/migrate/main.go",
            "args": [
                "-m",
                "refresh"
            ]
        }
    ]
}